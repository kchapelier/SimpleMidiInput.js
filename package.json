{
  "name": "simple-midi-input",
  "version": "1.2.1",
  "description": "Abstraction over the MIDI input. Does all the byte crunching and exposes a straightforward event API with MIDI learn capability.",
  "main": "index.js",
  "scripts": {
    "test": "gulp test",
    "build": "browserify index.js -d --s SimpleMidiInput -u crypto > build/SimpleMidiInput.js",
    "watch": "watchify index.js --s SimpleMidiInput -u crypto -o build/SimpleMidiInput.js -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kchapelier/SimpleMidiInput.js.git"
  },
  "author": "Kevin Chapelier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kchapelier/SimpleMidiInput.js/issues"
  },
  "homepage": "https://github.com/kchapelier/SimpleMidiInput.js",
  "devDependencies": {
    "browserify": "17.0.0",
    "chai": "^4.3.7",
    "gulp": "^4.0.2",
    "gulp-jshint": "^2.1.0",
    "gulp-mocha": "^8.0.0",
    "jshint-stylish": "^2.2.1"
  }
}
